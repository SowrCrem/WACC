# simple example class implementation

# Output:
# Aaron
# Fatty
#

# Exit:
# 0

# Program:

begin
	class animal is
		private string name;
		private int weight
		
		public constructor animal(string newName, int newWeight) is
			name = newName;
			weight = newWeight
		end
		
		public string getName() is
			return name
		end
		
		public void setName(string newName) is
			name = newName
		end
	end
	
	animal Aaron = new animal("Aaron", 100);
	string name = call Aaron.getName();
	println name;
	call Aaron.setName("Fatty");
	name = Aaron.getName();
	println name
	
end